module tb;
  int array[];
  initial begin
    array=new[3];
    array[0]=2;
    array[1]=3;
    array[2]=4;
    array[3]=6;
    array[4]=9;
    
    
    foreach(array[i])
      $display("array[%0d]=%0d",i,array[i]);
    $display("array_sieze=%0d",array.size());
    
    
    
  end
    

    
endmodule
